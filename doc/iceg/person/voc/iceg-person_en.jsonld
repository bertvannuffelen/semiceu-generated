  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Afstamming",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Descent is the descending line of blood relationship between different generations."
      },
      "usage": {
        "en": "The descent can be biological as well as non-biological, eg adoption. Descent can also be viewed in the ascending line. The concept is narrower than kinship that also includes marriage. There must be a direct link between the persons (no link between child and grandparent)."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Persoonsrelatie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#BurgerlijkeStaat",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Status of a person according to civil law."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Domicilie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Primary residence of a person."
      },
      "usage": {
        "en": "Place where the person resides most of the time within the jurisdiction of which he is a resident. This is usually officially established and registered in the population register. May differ from the actual place of residence."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Verblijfplaats"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Geboorte",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The person being born."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Persoonsgebeurtenis"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#GeenInwoner",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Person not residing in a certain place or country."
      },
      "usage": {
        "en": "Place or country is represented here by the jurisdiction entity."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Inwonerschap"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#GeregistreerdPersoon",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Person whose details are officially listed in a register."
      },
      "usage": {
        "en": "This register is usually a population register, but it can also be an electoral register, for example. The registered data relates to the identity (e.g. surname and first name) and the place of residence of the person and to important life events such as birth, marriage, decease, etc. These data are typically registered by the government, they offer the registered person legal protection and they allow the government to compile basic statistics about its population."
      },
      "parents": [
        "http://www.w3.org/ns/person#Person"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Gezin",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Form of cooperation that forms a recognizable social unit (eg through marriage), with persons who are blood relatives or not, who have lasting ties and provide mutual support and care."
      },
      "usage": {
        "en": "It is a broader concept than family where the family consists not solely of close relatives, eg a single-person family or living separately together. Household members usually share the same residence."
      },
      "parents": [
        "http://www.w3.org/ns/org#Organization"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Gezinsrelatie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Relation between members of the same household."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Persoonsrelatie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Huwelijk",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A form of cohabitation of two persons, organized by civil or religious law."
      },
      "usage": {
        "en": "Can, just like living together, form the basis of a household."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Persoonsrelatie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Inwoner",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Person who lives in a certain place or country."
      },
      "usage": {
        "en": "This place or country is represented here by the jurisdiction entity."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Inwonerschap"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Inwonerschap",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The fact that a person resides in a place or country."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Nationaliteit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The relationship between a person and a state, culture or loyalty."
      },
      "usage": {
        "en": "Note that this concept is broader than citizenship (which is limited to the relationship with a state). Anyone who has the nationality of a particular country is usually automatically a citizen. Nationality is usually obtained automatically by birth: one gets either the nationality of his parents or one gets it from the country where one is a resident. One can also obtain a certain nationality through naturalization. In principle, peoplethen lose their original nationality, but that is not always the case (multiple nationality)."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Overlijden",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "death of a person."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Persoonsgebeurtenis"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#PermanentInwoner",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Person who permanently resides in a particular place or country."
      },
      "usage": {
        "en": "Is a right of residence that must in principle be officially granted if the person is not a citizen."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Inwoner"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Persoonsgebeurtenis",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Significant event in the life of a person."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Persoonsrelatie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Relation between two or more persons."
      },
      "usage": {
        "en": "Typically these are civil law relationships (see civil status) but not necessarily limited thereto."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Samenwonen",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Arrangement whereby two people who are not married live together. Can, just like a marriage, form the basis of a household. Can be legally registered."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Persoonsrelatie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Staatsburger",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Person legally associated with a state."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Staatsburgerschap"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Staatsburgerschap",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Legal connection of a Person with a state, with which certain rights and obligations are attached."
      },
      "usage": {
        "en": "Usually derives from the Nationality of the Person. The rights and obligations in question are civil rights (eg freedom of religion) or of a political (eg right to vote) or social nature (eg right to benefits)."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#TijdelijkInwoner",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Person who temporarily lives in a place or country."
      },
      "usage": {
        "en": "Is a right of residence that is in principle only granted for very specific reasons, eg work or study. Does not apply to a short stay, eg as a tourist."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Inwoner"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Verblijfplaats",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Place where a Person lives or stays permanently or temporarily."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Voogdij",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Situation where a person acting as guardian serves another person and in that capacity is responsible for this person and his goods."
      },
      "usage": {
        "en": "This is the case, for example, for minors who have lost their parents or for adults who have lost their intellectual faculties."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Persoonsrelatie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/persoon#Vreemdeling",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Person who does not have the nationality of the country where he resides."
      },
      "usage": {
        "en": "Therefore, the person cannot be called a citizen. As a resident however the foreigner enjoys at least a subset of the civil rights that a citizen has. Freedom of religion, for example, but not necessarily the right to vote."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/persoon#Staatsburgerschap"
      ]
    }
  ],
