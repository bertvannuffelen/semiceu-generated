  "classes": [
    {
      "@id": "http://vocab.belgif.be/ns/building#Building",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "An enclosed and/or covered structure, above and/or below ground, intended either for the shelter of persons, animals or things, or for the production of economic goods or the provision of services, and refers to any structure permanently constructed or erected on its site."
      },
      "parents": [
        "http://www.w3.org/2000/01/rdf-schema#Resource"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "This data types includes the geometry of the building and metadata information about which element of the building was captured and how."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry2D",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "This data types includes the geometry in 2D or 2.5D of the building and metadata information about which element of the building was captured and how."
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Data type grouping the 3D geometry of a building or building part and the metadata information attached to this geometry."
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD1",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Data type grouping the specific metadata attached to the 3D geometry, when provided by a LoD 1 representation."
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD2",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Data type grouping the specific metadata attached to the 3D geometry, when provided by a LoD2 representation."
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingNatureValue",
