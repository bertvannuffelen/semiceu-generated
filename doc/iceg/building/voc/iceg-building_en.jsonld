      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingUnit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The smallest unit within a building that is suitable and adapted for residential, commercial or recreational purposes and which is accessible from the outside or from a common area. A building unit is atomic, functionally independent, and may be separately sold, rented out, inherited, etc. In addition, a building unit can also be a common part."
      },
      "parents": [
        "http://www.w3.org/2000/01/rdf-schema#Resource"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#ConditionOfConstructionValue",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Values indicating the condition of a construction."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#Elevation",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Vertically-constrained dimensional property consisting of an absolute measure referenced to a well-defined surface which is commonly taken as origin (geoïd, water level, etc.)."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#ElevationReferenceValue",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#EntranceLocation",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The location where the building unit can be entered."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#EventDate",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "This data type includes the different possible ways to define the date of an event."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#FunctionValue",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#HorizontalGeometryReferenceValue",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#Length",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The measure of distance as an integral, i.e. the limit of an infinite sum of distances between points on a curve.  For example the length of curve, the perimeter of a polygon as the length of the boundary."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#MeasuringMethod",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "TO DO"
      },
      "parents": []
    }
  ],
