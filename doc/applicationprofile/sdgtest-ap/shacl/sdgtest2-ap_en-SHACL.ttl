<https://semiceu.github.io/shacl/sdgtest2-ap_en#CourseResultShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class skos:Concept;
    shacl:definition "Main language in which the course was taught."@en;
    shacl:maxCount 1;
    shacl:name "course language"@en;
    shacl:path <https://sdg.semic.eucourseLanguage>
  ], [
    shacl:definition "Period of the academic year when the course took place, e.g. first semester, second trimester, etc."@en;
    shacl:maxCount 1;
    shacl:name "course academic period"@en;
    shacl:path <https://sdg.semic.eucourseAcademicPeriod>
  ], [
    shacl:class <https://sdg.semic.euGrade>;
    shacl:definition """ 	A mark indicating a degree of accomplishment for a particular course."""@en;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "course grade"@en;
    shacl:path <https://sdg.semic.eucourseGrade>
  ], [
    shacl:class skos:Concept;
    shacl:definition "The discipline or subject area of a course."@en;
    shacl:name "course field of study"@en;
    shacl:path <https://sdg.semic.eucourseFieldOfStudy>
  ], [
    shacl:datatype xsd:date;
    shacl:definition """ 	The annual period of sessions of an educational programme usually beginning in September and ending in June."""@en;
    shacl:maxCount 1;
    shacl:name "course academic period"@en;
    shacl:path <https://sdg.semic.eucourseAcademicYear>
