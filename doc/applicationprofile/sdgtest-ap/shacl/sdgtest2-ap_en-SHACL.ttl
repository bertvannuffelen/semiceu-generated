    shacl:definition """ 	The complete address written as a string, with or without formatting."""@en;
    shacl:maxCount 1;
    shacl:name "full address"@en;
    shacl:path <https://sdg.semic.eufullAddress>
  ], [
    shacl:maxCount 1;
    shacl:name "admin unit Level 2"@en;
    shacl:path <https://sdg.semic.euadminUnitL2>
  ], [
    shacl:class <https://sdg.semic.euLocation>;
    shacl:path <https://sdg.semic.euAddress.address>
  ], [
    shacl:class <https://sdg.semic.euLocation>;
    shacl:path <https://sdg.semic.euAddress.address>
  ], [
    <http://purl.org/linked-data/cube#codeList> <http://publications.europa.eu/resource/authority/country>;
