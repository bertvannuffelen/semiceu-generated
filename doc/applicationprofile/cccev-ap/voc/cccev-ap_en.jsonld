  "classes": [
    {
      "@id": "http://data.europa.eu/m8g/cccev#Concept",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A reference to an entity, i.e. a class or a property, which used to describe information in the to be provided evidence for the requirement"
      },
      "parents": []
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#Constraint",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Limitation applied to requirement(s) or to the concept(s) the requirement is about."
      },
      "usage": {
        "en": "An example in eProcurement, a threshold: the minimum Turnover required by the buyer to select the candidates."
      },
      "parents": [
        "http://data.europa.eu/m8g/cccev#Requirement"
      ]
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#Criterion",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "condition for evaluation or assessment."
      },
      "usage": {
        "en": "In general, criteria are used for comparison, filtering and selection. Criteria usually set minimum constraints (e.g. limits, intervals, thresholds, etc.) that need to be met in order to pass the required conditions or to fulfil them in a certain degree or quality. The evaluation of the fulfilment is usually supported with the provision of evidences."
      },
      "parents": [
        "http://data.europa.eu/m8g/cccev#Requirement"
      ]
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#Evidence",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The data proving that a requirement is met or has been fulfilled."
      },
      "parents": [
        "http://www.w3.org/ns/dcat#Dataset"
      ]
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#EvidenceType",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Information about the characteristics of an expected evidence, such as the desired content, format, source, etc."
      },
      "parents": []
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#EvidenceTypeList",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A combination of evidence types that must be satisfied all together."
      },
      "usage": {
        "en": "NOTE: An evidence type list is satisfied iff for all evidence types an corresponding conformant evidence is supporting the requirement having this list."
      },
      "parents": []
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#InformationRequirement",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A request for data that is proof of evidence or leads to the source of such a proof."
      },
      "parents": [
        "http://data.europa.eu/m8g/cccev#Requirement"
      ]
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#ReferenceFramework",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A source from where requirements are identified and drawn."
      },
      "parents": []
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#Requirement",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A condition demanded."
      },
      "parents": []
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#SupportedValue",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A value for an information concept that is supported by an Evidence"
      },
      "parents": []
    }
  ],
