    {
      "@id": "http://data.europa.eu/m8g/cccev#Constraint",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "de": "Beschränkung, die auf die Anforderungen oder die Konzepte angewendet wird, um die sich die Anforderung richtet."
      },
      "usage": {
        "de": "Ein Beispiel beim eProcurement, ein Schwellenwert: der Mindestumsatz, den der Käufer für die Auswahl der Kandidaten benötigt."
      },
      "parents": [
        "http://data.europa.eu/m8g/cccev#Requirement"
      ]
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#Criterion",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "de": "Voraussetzung für die Bewertung oder Bewertung."
      },
      "usage": {
        "de": "Im Allgemeinen werden Kriterien für Vergleich, Filterung und Auswahl verwendet. Kriterien legen in der Regel Mindestbeschränkungen fest (z. B. Grenzwerte, Intervalle, Schwellenwerte usw.), die erfüllt sein müssen, um die erforderlichen Bedingungen zu erfüllen oder sie in einem bestimmten Grad oder einer bestimmten Qualität zu erfüllen. Die Bewertung der Erfüllung wird in der Regel durch die Bereitstellung von Nachweisen unterstützt."
      },
      "parents": [
        "http://data.europa.eu/m8g/cccev#Requirement"
      ]
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#Evidence",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "de": "Die Daten, welche belegen, dass eine Anforderung erfüllt oder erfüllt ist."
      },
      "parents": [
        "http://www.w3.org/ns/dcat#Dataset"
      ]
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#EvidenceType",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "de": "Informationen über die Merkmale eines erwarteten Nachweises, z. B. den gewünschten Inhalt, das Format, die Quelle usw."
      },
      "parents": []
    },
    {
      "@id": "http://data.europa.eu/m8g/cccev#EvidenceTypeList",
