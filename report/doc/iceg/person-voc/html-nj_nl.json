      ]
    },
    {
      "uri": "http://vocab.belgif.be/ns/person#Domicilie",
      "name": "Domicile",
      "label": {
        "en": "Domicile",
        "nl": "Enter your translation here"
      },
      "sort": "Enter your translation here",
      "description": {
        "en": "Primary residence of a person.",
        "nl": "Enter your translation here"
      },
      "usage": {
        "en": "Place where the person resides most of the time within the jurisdiction of which he is a resident. This is usually officially established and registered in the population register. May differ from the actual place of residence.",
        "nl": "Enter your translation here"
      },
      "equivalent": [],
      "parents": [
        "http://vocab.belgif.be/ns/person#residence"
