        {
          "uri": "http://www.w3.org/ns/legal#companyStatus",
          "name": "legal entity status",
          "sort": "legal entity status",
          "label": {
            "en": "legal entity status"
          },
          "description": {
            "en": "This property records the status of the Legal Entity."
          },
          "usage": {
            "en": "Recording the status of a company presents the same issues as its type. The terms 'insolvent', 'bankrupt' and 'in receivership,' for example, are likely to mean slightly different things with different legal implications in different jurisdictions. Best practice for recording various other status levels is to use the relevant jurisdiction's terms and to identify the controlled vocabulary used."
          },
          "domain": [
            {
              "EA-Name": "Legal Entity",
              "uri": "http://www.w3.org/ns/legal#LegalEntity"
